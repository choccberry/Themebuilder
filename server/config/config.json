{
  "development": {
    "username": "process.env.DB_USERNAME",
    "password": "process.env.DB_PASSWORD",
    "database": "process.env.DB_NAME",
    "host": "127.0.0.1",
    "dialect": "postgres"
  },
  "test": {
    "username": "process.env.DB_USERNAME",
    "password": "process.env.DB_PASSWORD",
    "database": "process.env.DB_NAME_TEST",
    "host": "127.0.0.1",
    "dialect": "postgres"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}
